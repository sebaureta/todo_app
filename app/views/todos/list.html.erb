<div class= "container">
    <div class= "row">
  <div class="col-md-6">

<h1>Completados</h1>

<table>
  <thead>

      <th colspan="3"></th>

  </thead>

  <tbody>
    <% @completed.each do |todo| %>

       <tr> <td><%=todo.description%></td></tr>

    <% end %>
  </tbody>
</table>

<br>



  </div>
  <div class="col-md-6">
  <h2>Incompletados</h2>
<table>
  <thead>
      <th colspan="3"></th>

  </thead>

  <tbody>
    <% @nocompleted.each do |todo| %>

       <tr> <td><%=todo.description%></td></tr>

    <% end %>
  </tbody>
</table>

<br>



  </div>

</div>
</div>
